{
    "name": "ucpp",
    "displayName": "uC++",
    "description": "Syntax highlighting for uC++.",
    "publisher": "DillionVerma",
    "repository": {
        "type": "git",
        "url": "https://github.com/dillionverma/ucpp.git"
    },
    "version": "1.0.3",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ucpp",
                "aliases": [
                    "uC++",
                    "ucpp"
                ],
                "extensions": [
                    ".cc",
                    ".ucc",
                    ".ucpp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ucpp",
                "scopeName": "source.ucpp",
                "path": "./syntaxes/ucpp.tmLanguage"
            }
        ]
    }
}
