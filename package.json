{
    "name": "ucpp",
    "displayName": "uC++ Lang",
    "description": "Syntax highlighting for uC++ lang",
    "publisher": "DillionVerma",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ucpp",
            "aliases": ["uC++", "ucpp"],
            "extensions": [".cc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ucpp",
            "scopeName": "source.ucpp",
            "path": "./syntaxes/ucpp.tmLanguage"
        }]
    }
}